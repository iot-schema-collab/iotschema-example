    {
      "@id": "iot:writable",
      "@type": "rdf:Property",
      "rdfs:label": "writable",
      "rdfs:comment": "Property to specify writability of a property.",
      "schema:domainIncludes": {
        "@id": "iot:Property"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Boolean"
        }
      ]
    },
    {
      "@id": "iot:observable",
      "@type": "rdf:Property",
      "rdfs:label": "observable",
      "rdfs:comment": "Property to specify observability of a property.",
      "schema:domainIncludes": {
        "@id": "iot:Property"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Boolean"
        }
      ]
    }
  ]
}