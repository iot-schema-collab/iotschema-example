      "rdfs:comment": "Capability Class",
      "rdfs:label": "Capability",
      "rdfs:subClassOf": {
        "@id": "schema:Thing"
      }
    },
    {
      "@id": "iot:isAssociatedWith",
      "@type": "rdf:Property",
      "rdfs:comment": " A relation between a Capability and the entity it belongs to.",
