      "rdfs:label": "PropertyChangedEvent",
      "rdfs:subClassOf": {
        "@id": "iot:Event"
      }
    },
    {
      "@id": "iot:Building",
      "@type": "iot:Domain",
      "rdfs:comment": "Building Domain",
      "rdfs:label": "Building"
    },
    {
      "@id": "iot:Industry",
      "@type": "iot:Domain",
      "rdfs:comment": "Industry Domain",
      "rdfs:label": "Industry"
    },
    {
      "@id": "iot:Mobility",
      "@type": "iot:Domain",
      "rdfs:comment": "Mobility Domain",
      "rdfs:label": "Mobility"
    },
    {
      "@id": "iot:madeObservation",
      "@type": "rdf:Property",
      "rdfs:comment": " Relation between a Sensor and a PropertyValue it has made. ",
      "rdfs:label": "madeObservation",
      "schema:domainIncludes": {
        "@id": "iot:Sensor"
      },
      "schema:rangeIncludes": {
        "@id": "schema:PropertyValue"
      }
    },
    {
      "@id": "iot:Device",
      "@type": "rdfs:Class",
      "rdfs:comment": "Device Class",
      "rdfs:label": "Device",
      "rdfs:subClassOf": {
        "@id": "schema:Thing"
      }
