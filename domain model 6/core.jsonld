    {
      "@id": "iot:capability",
      "@type": "rdf:Property",
      "rdfs:label": "capability",
      "rdfs:comment": "A property that relates an interaction pattern with its capability .",
      "schema:domainIncludes": {
        "@id": "iot:InteractionPattern"
